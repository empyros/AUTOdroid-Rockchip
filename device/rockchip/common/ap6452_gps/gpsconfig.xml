       LogFacMask="LOG_GLLAPI | LOG_DEVIA | LOG_NMEA | LOG_RAWDATA | LOG_DEVMS | LOG_ASIC_IO | LOG_BBTEST | LOG_DEVET | LOG_MESM | LOG_DEVKF | LOG_DEVJG | LOG_DEVMR"
       FrqPlan="FRQ_PLAN_26MHZ_2PPM"
       RfType="GL_RF_4752_BRCM_EXT_LNA"
  />

</glgps>
